{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "legacyDecorators": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "react/prop-types": 0
    },
    "globals": {
        "__DEV__": false
    },
    "settings": {
        "react": {
            "version": "16.0"
        }
    }
}
